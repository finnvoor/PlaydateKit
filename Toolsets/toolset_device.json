{
	"schemaVersion": "1.0",
	"swiftCompiler": {
		"extraCLIOptions": [
			"-target", "armv7em-none-none-eabi",
			"-Xfrontend", "-experimental-platform-c-calling-convention=arm_aapcs_vfp"
		]
	},
	"cCompiler": {
		"extraCLIOptions": [
			"-target", "armv7em-none-none-eabi",

			"-mthumb",
			"-mcpu=cortex-m7",
			"-mfloat-abi=hard",
			"-mfpu=fpv5-sp-d16",
			"-D__FPU_USED=1",
			"-DTARGET_PLAYDATE=1",

			"-falign-functions=16",
			"-fshort-enums"
		]
	}
}
